<script type="text/javascript"><!--

var canvas = document.getElementById('canvas_picker').getContext('2d');

// create an image object and get it’s source
var img = new Image();
img.src = '{{ base }}/image/catalog/colors.jpg';

// create code array variable
var colors = new Array();
var colors = ["f8f7f5", "f8f7f5", "f8f7f5", "f8f7f5", "f8f7f5", "f8f7f5", "f8f7f5", "f8f7f5", "f8f7f5", "f8f7f5", "f8f7f5", "f8f7f5", "f8f7f5", "f8f7f5", "f8f7f5", "f8f7f5", "f8f7f5", "f8f7f5", "f8f7f5", "f8f7f5", "f8f7f5", "f8f7f5", "f8f7f5", "f8f7f5"];

// copy the image to the canvas
$(img).load(function () {
    canvas.drawImage(img, 0, 0);
});


// http://www.javascripter.net/faq/rgbtohex.htm
function rgbToHex(R, G, B) {
    return toHex(R) + toHex(G) + toHex(B)
}

function toHex(n) {
    n = parseInt(n, 10);
    if (isNaN(n)) return "00";
    n = Math.max(0, Math.min(n, 255));
    return "0123456789ABCDEF".charAt((n - n % 16) / 16) + "0123456789ABCDEF".charAt(n % 16);

}

$('#canvas_picker').click(function (event) {

    // getting user coordinates
    //var x = event.pageX - this.offsetLeft;
    //var y = event.pageY - this.offsetTop;
    var x = event.offsetX;
    var y = event.offsetY;
    // getting image data and RGB values
    console.log(event);
    var img_data = canvas.getImageData(x, y, 1, 1).data;
    var R = img_data[0];
    var G = img_data[1];
    var B = img_data[2];
    var rgb = R + ',' + G + ',' + B;

    // convert RGB to HEX
    var hex = rgbToHex(R, G, B);


    // making the color the value of the input by matching checkbox values to css

    var editedColor = document.getElementById('superform').editedColor;
    var i;
    for (i = 0; i < editedColor.length; i++) {
        if (editedColor[i].checked) {
            colors[i] = hex
            $("." + [i]).css("background-color", '#' + colors[i]);
            $("#copyButton_" + [i]).css("background-color", '#' + colors[i]);
            $("#lineColor_" + [i]).css("background-color", '#' + colors[i]);
        }
    }


    $('#rgb input').val(rgb);
    $('#hex input').val('#' + hex);
    $('#x input').val(x);
    $('#y input').val(y);

    // passing color to php via form
    console.log(colors);
    document.getElementById('input-option416').value = colors;


});

// Displaying actual colorcode in input field by making colorcode string out of colors array
function displayColorcode() {
    colorcode = colors.join();
    $('#colorcode input').val(colorcode);
    document.getElementById('codefield').select();
}

// Recieving color code from input and displaying cable by spliting gotCode string into displayCode array
function recieveColorcode() {
    var nameElement = document.getElementById("codefield");
    var gotCode = nameElement.value;

    var displayCode = gotCode.split(',');

    var i;
    for (i = 0; i < displayCode.length; i++) {
        $("." + [i]).css("background-color", '#' + displayCode[i]);
        $("#copyButton_" + [i]).css("background-color", '#' + displayCode[i]);
        $("#lineColor_" + [i]).css("background-color", '#' + displayCode[i]);
        colors[i] = displayCode[i];
    }
}


function toggle(source) {
    checkboxes = document.getElementsByName('editedColor');
    for (var i = 0, n = checkboxes.length; i < n; i++) {
        checkboxes[i].checked = source.checked;
    }
}

function invertSelection() {
    var editedColor = document.getElementById('superform').editedColor;
    var i;
    for (i = 0; i < editedColor.length; i++) {
        if (editedColor[i].checked) {
            editedColor[i].checked = false;

        } else {
            editedColor[i].checked = true;

        }
    }

}

function copyColor(intValue) {
    var editedColor = document.getElementById('superform').editedColor;
    var copyButton = document.getElementById('superform').copyButton;
    var i;

    for (i = 0; i < editedColor.length; i++) {
        if (editedColor[i].checked) {
            var q = document.getElementsByName("copyButton").value;
            colors[i] = colors[intValue]
            $("." + [i]).css("background-color", '#' + colors[i]);
            $("#copyButton_" + [i]).css("background-color", '#' + colors[i]);
            $("#lineColor_" + [i]).css("background-color", '#' + colors[i]);

        }
    }
}

