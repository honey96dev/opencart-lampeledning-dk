{{ header }}

<style>

[class*="product-cabledesign"]  hr {
    display: none;
}
[class*="product-cabledesign"] #product {
    margin-top:15px;
}

  
[class*="product-cabledesign"] #product h3 {
    font-size: 14px;
    margin-bottom: 20px;
    font-weight: bold;
  color: #545454;
}
[class*="product-cabledesign"] #product .form-control  {
    display: inline-block;
    font-size: 14px;
    line-height: 2;
    color: black;
    background-color: #fff;
    border: 1px solid #B5B5B5;
    border-radius: 0;
    -webkit-transition: none;
    -o-transition: none;
    transition: none;
}
#input-quantity {
  width:100px;
}
[class*="product-cabledesign"] #product  label {
    display:block;
}

[class*="product-cabledesign"] #product .radio {
    display:inline-block;
}
[class*="product-cabledesign"] #product .radio label {
    font-size:0px; 
  line-height: 24px;
  margin-right: 5px;
  padding-left: 0px;
}
[class*="product-cabledesign"] #product .radio input[type="radio"] {
    display: none;  
}
[class*="product-cabledesign"] #product .radio input[type="radio"]+img{
    background:none;
  border-radius:0;
  -webkit-border-radius:0;
  border-color:#F4F4F4;
  background-position:center;
}
[class*="product-cabledesign"] .radio input[type="radio"]:checked+img{ 
    border: solid 1px #B5B5B5 !important; 
  background: url("https://juheko.com/shop/image/crossection-bg.png") center !important;
} 
[class*="product-cabledesign"] [id*="input-option"] {
    display: inline-block;
  margin-top: -10px;
}
[class*="product-cabledesign"] .radio input[type=radio] {
    margin-left: -16px;
}
[class*="product-cabledesign"] #product .btn-primary {
    margin-top: 26px;
    color: #000000;
    text-shadow:none ; 
    background-color: #F7DF21;
    background-image: none;
    border: solid 4px black;
    border-radius: 0;
    font-family: code_boldregular, 'Josefin Sans', sans-serif;
    font-size: 22px;
    letter-spacing: 2px;
    text-transform: uppercase;
}
[class*="product-cabledesign"] #product .btn-primary:hover{
    color: #F7DF21;
    background-color: #000000;
}

</style>

<style type="text/css">
      
        


#container {margin: 20px auto; padding:20px; background:#F4F4F4; max-width:880px; height: 80%px; overflow:hidden; 
     }

#cableContainer {display:block; position: relative; width: 480px; height: 250px; overflow: hidden; padding-left: 10px; }    
#colorChart {display:block; overflow:hidden; width:360px; float:left;}
#usedColors {display:block; }
#usedColors span{font-size:12px;} 
#manipulatorArea{display:block;}
#colorCodeArea {display:block;}   
#cable {margin-right:60px; margin-top:-50px; margin-left: 10px; background:grey; position:relative; float:left;  transform:rotate(125deg); -ms-transform:rotate(125deg); /* IE 9 */ -webkit-transform:rotate(125deg); }
#overlayTriangle1{width: 0px; height: 0px; margin-left:-1px; margin-top:-1px; border-style: solid; border-width: 88px 88px 0 0; border-color: #F4F4F4 transparent transparent; position:absolute; z-index:1000;}
#overlayTriangle2{ right:0px; margin-right:-1px; margin-top:-1px; border-style: solid; border-width: 0 366px 366px 0; border-color: transparent #F4F4F4 transparent transparent; position:absolute; z-index:1000;}
#overlayTriangle3{ right:0px; bottom:0px; margin-right:-1px; margin-bottom:-1px; border-style: solid; border-width: 0 0 88px 88px; border-color: transparent transparent #F4F4F4 transparent; position:absolute; z-index:1000;}
#overlayTriangle4{ bottom:0px; width: 0px; height: 0px; border-style: solid; border-width: 366px 0 0 366px; border-color: transparent transparent transparent #F4F4F4; position:absolute; margin-left:-1px; margin-bottom:-1px; z-index:1000;}


#overlay {width:450px;  height:38px;  -webkit-box-shadow: inset 0px 0px 11px 0px rgba(0,0,0,0.45);  -moz-box-shadow: inset 0px 0px 11px 0px rgba(0,0,0,0.45); box-shadow: inset 0px 0px 11px 0px rgba(0,0,0,0.45);  transform:rotate(45deg);  -ms-transform:rotate(45deg); /* IE 9 */ -webkit-transform:rotate(45deg); /* Opera, Chrome, and Safari */  position:absolute;  bottom: 175px;  left: -30px;  z-index:1000;}  
#overlayText{ width: 0px; height: 0px; margin-left:-1px; margin-top:-1px; position:absolute; z-index:1001;}   
.rowh1 {margin-left:0px;}
.rowh2 {margin-left:4px;}
.rowh3 {margin-left:8px;}
.rowh4 {margin-left:12px;}
.rh {width: 8px;  height: 4px;  background: #f8f7f5; margin-right:8px;  float:left;}
.rowv1 {margin-top:4px; float:left; position:relative;}
.rowv2 {margin-top:8px; float:left;  position:relative;}
.rowv3 {margin-top:12px; float:left;  position:relative;}
.rowv4 {margin-top:16px; float:left;  position:relative;}
.rv {width: 4px;  height: 8px;  background: #f8f7f5; margin-bottom:8px;}


form div.choose button{background: #f8f7f5;
    color: black;
    border: 0;
    height: 17px;
    width: 17px;
    margin: 2px 0 0 5px;
    float: right;
    cursor: auto;
     -webkit-border-radius: 17px;
    -moz-border-radius: 6px;
    border-radius: 17px;  }
.colorButton {background: #f8f7f5;  color:black;  border:0px; padding:0px;  height:16px;  width:16px; margin-right:1px; margin-left:0px;  cursor:crosshair; font-size:12px; -webkit-borderradius:8px;       
  -moz-border-radius:8px; border-radius:8px;  }
.otherButton {  background-color:#fffffff;  -webkit-borderradius:0;   -moz-border-radius:0px; border-radius:0px;  margin-right:20px;  border:1px solid #333333; display:inline-block; color:rgb(100,100,100); font-size:13px; font-weight:normal; font-style:normal;  height:24px;  line-height:21px; width:auto; text-decoration:none; text-align:center;  }
.otherButton:hover {  background:-webkit-gradient( linear, left top, left bottom, color-stop(0.05, #dfdfdf), color-stop(1, #ededed) );  background:-moz-linear-gradient( center top, #dfdfdf 5%, #ededed 100% );  filter:progid:DXImageTransform.Microsoft.gradient(startColorstr='#dfdfdf', endColorstr='#ededed');  background-color:#dfdfdf;}
.quoteButton {  background-color: rgb(205, 237, 126); -webkit-borderradius:6px; -moz-border-radius:6px; border-radius:6px;  -webkit-border-top-right-radius:6px;  margin-right:30px;  border:1px solid #dcdcdc; display:inline-block; color:rgb(40,40,40);  font-family:arial;  font-size:13px; font-weight:normal; font-style:normal;  float:left; height:32px;  line-height:30px; width:auto; text-decoration:none; text-align:center;  }

.choose {float:left;  margin-right:30px;  margin-left:8px;}
.choose span {  margin-right:7px; width:10px; display:inline-block;}
.choose input[type="checkbox"] {    right: 0px;
    margin: 5px 4px;
    display: inline;
    cursor: pointer;  }
  
@media only screen and (max-width: 880px) {
body{font-size:16px;}
.choose span {line-height:25px;}
#cableContainer{float:none;}
#container{width:480px;}
}
  
@media only screen and (max-width: 500px) {

#container{width:320px; overflow:hidden;}
}

@media (min-width: 768px) {
.fake-8  {
    width: 66.66666667%;
  float: left;
}}

</style>

